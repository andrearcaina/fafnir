type LoginResponse {
    code: Int
    message: String
    error: String
}

type RegisterResponse {
    code: Int,
    message: String,
    error: String
}

input RegisterRequest {
    email: String!
    password: String!
}

input LoginRequest {
    email: String!
    password: String!
}

type Mutation {
    login(input: LoginRequest!): LoginResponse!
    register(input: RegisterRequest!): RegisterResponse!
}